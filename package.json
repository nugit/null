    "scripts": {
      "lint": "eslint --cache . --ext .js,.jsx,.flow",
      "lint:friendly": "eslint --cache . --ext .js,.jsx,.flow --format 'node_modules/eslint-formatter-friendly'",
      "test": "cross-env TZ='Asia/Singapore' jest",
      "test-update-snapshots": "cross-env TZ='Asia/Singapore' jest -u",
      "bench": "cross-env TZ='Asia/Singapore' jest --config jest.bench.config.js",
      "tdd": "cross-env TZ='Asia/Singapore' jest --watch",
      "coverage": "cross-env TZ='Asia/Singapore' jest --coverage",
      "dev": "cross-env BABEL_ENV=server nodemon ./src/server/server.dev.js",
      "dev:debug": "BABEL_ENV=server node --max-old-space-size=3072 --inspect ./src/server/server.dev.js",
      "start": "cross-env BABEL_ENV=client webpack --config config/webpack.config.dev.js --progress",
      "build": "cross-env BABEL_ENV=client webpack --config config/webpack.config.prod.js",
      "compile": "yarn compile:legacy && yarn compile:server",
      "compile:legacy": "cross-env BABEL_ENV=server babel source --out-dir dist/source --copy-files --ignore source/**/*__test.js --source-maps",
      "compile:server": "cross-env BABEL_ENV=server babel src --out-dir dist/src --copy-files --ignore source/**/*__test.js --source-maps",
      "storybook": "start-storybook -p 9001 -c .storybook -s ./public",
      "storybook:build": "build-storybook -s public -c .storybook",
      "doc:api:update": "widdershins --omitBody -c src/server/api/doc/api.yaml src/server/api/doc/api.md"
    },
    "engines": {
      "node": "12.10.x"
    },
    "bundlesize": [
      {
        "path": "./public/scripts/sdk.js",
        "maxSize": "3 kB"
      },
      {
        "path": "./public/scripts/vendor~vis.*.chunk.js",
        "maxSize": "90 kB"
      },
      {
        "path": "./public/scripts/dataNugits~vis.*.chunk.js",
        "maxSize": "100 kB"
      },
      {
        "path": "./public/scripts/generalNugits~vis.*.chunk.js",
        "maxSize": "10 kB"
      },
      {
        "path": "./public/scripts/vis.*.chunk.js",
        "maxSize": "70 kB"
      },
      {
        "path": "./public/scripts/*.css",
        "maxSize": "30 kB"
      }
    ],
    "config": {
      "pre-git": {
        "allow-untracked-files": true,
        "pre-push": [
          "yarn lint",
          "yarn test"
        ]
      }
    },
    "nodemonConfig": {
      "watch": [
        "package.json",
        "server.js",
        "node_modules/**/*",
        "source/**/*.js",
        "source/**/*.json",
        ".env"
      ],
      "ignore": [
        "source/**/test/**/*",
        "source/**/__test/**/*"
      ]
    },
    "release": {
      "analyzeCommits": "simple-commit-message"
    }
  }
  